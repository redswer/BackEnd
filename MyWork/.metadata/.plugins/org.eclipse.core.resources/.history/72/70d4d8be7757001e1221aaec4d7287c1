public class Lm04_FunctionTest {

	public static void main(String[] args) {
		// 1. String 의 길이 return

		Function<String, Integer> f1 = s -> s.length();

//		======================
		// 2. Double return

		Double db = new Double();

		db.dbMul((a, b) -> a * b);

	}// main

}// class