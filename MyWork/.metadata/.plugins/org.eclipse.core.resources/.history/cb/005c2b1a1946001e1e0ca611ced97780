package j01_basic;

public class Ex06_Print {

	public static void main(String[] args) {
		// ** Escape 문자 : 역활이 정해져 있는 문자
		// => \n 줄바꿈, \t 탭간격, ...
		D:\JinHyuk_Ahn\BackEnd\MyWork

		// ** print, println, printf

		// ** 날짜출력
		// java.util.Date now

		// => 출력형식
		// % : 출력형식을 의미하는 문자열은 반드시 %로 시작,
		// %d : 정수 , %f 실수, %s 문자
		// %,d : 정수에 3자리마다 , 표시
		// %t : 날짜 시간을
		// %n , \n : 줄바꿈
		// $ 사용하면 매개변수의 갯수를 줄일 수 있음
		// => %2$d : 2번째 매개변수 정수
	}

}