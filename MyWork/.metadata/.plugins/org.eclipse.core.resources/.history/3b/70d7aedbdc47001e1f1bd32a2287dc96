package a01_homework;

public class Ex01_230831 {

	public static void main(String[] args) {
		// 4-7
		for (int i = 1; i <= 6; i++) {
			for (int j = 1; j <= 6; j++) {
				if (i + j == 6) {
					System.out.println(i + "+" + j + "=" + (i + j));
				}
			}
		}

//		=======================================
	} // main

} // class