	String color;
	int door;

	void drive() {
		System.out.println("drive, Brrrr~");
	}

	void stop() {
		System.out.println("Stop!!!");
	}
}

class FireEngine extends Car {
	void water() {
		System.out.println("Water!!!");
	}
}

public class Ex04_230911 {

	public static void main(String[] args) {
		Car car = null;
		FireEngine fe = new FireEngine();

	}

}
