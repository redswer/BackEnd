package j17_Lamda;

@FunctionalInterface
interface St {
	String st(String st1);
}

@FunctionalInterface
interface Db {
	double db(double db1, double db2);
}

class Str {
	public void sSize(St s) {
		int result = s.st("string").length();

		System.out.println(result);
	}
}

class Double {
	public void dbMul(Db b) {
		double result = b.db(123.456, 456.789);
	}
}

public class Lm04_FunctionTest {

	public static void main(String[] args) {
		// 1. String 의 길이 return

		Str str = new Str();

		str.sSize(a -> a);

		// 2. Double return

	}// main

}// class