package j04_array;

//** 다차원 배열
//1차원 배열이 2개 모이면 2차원 배열
//1차원 배열이 3개 모이면 3차원 배열 ...2차원 이상은 거의 안쓰임

public class Ex03_MultiArray {

	public static void main(String[] args) {
		// 1. 명시적 선언
		// => mul = {{10, 20, 30}, {11, 22, 33 }}

		int[][] mul = new int[2][3];

		mul[0][0] = 10;
		mul[0][1] = 20;
		mul[0][2] = 30;
		mul[1][0] = 11;
		mul[1][1] = 22;
		// mul[1][2] = 33;
		// => 배열에서 값을 할당하지 않으면 default 값을 가짐 -> int 의 디폴트값인 0

	} // main

} // class